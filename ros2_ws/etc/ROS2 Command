# git commands
git https://github.com/su1jun/Capstone.git
git pull https://github.com/su1jun/Capstone

# ros2 running
source /opt/ros/humble/setup.bash
source /usr/share/colcon_argcomplete/hook/colcon-argcomplete.bash
source ~/ros2_ws/install/setup.bash
source ~/.bashrc

# directory
cd /opt/ros/humble

# build
# have to move in directory
ros2 pkg create robot --build-type ament_python --dependencies rclpy
colcon build

# run
. install/setup.bash
ros2 run ros2test my_node

### packages
# graph
rqt_graph

# gazebo
gazebo --verbose

# turotial
ros2 run turtlesim turtlesim_node
ros2 run turtlesim turtle_teleop_key

### network
# wireless network driver
Realtek Semiconductor Co., Ltd. Device b852

# ssh service
/etc/init.d/ssh start && ssh su1jun@192.168.137.165

# ftp service
scp /path/to/local/file su1jun@192.168.137.69:/path/to/remote/directory # ->
scp su1jun@192.168.137.69:/path/to/remote/file /path/to/local/directory # <-

# hospot setting 
nmcli d wifi hotspot ifname <wifi_iface> ssid <ssid> password <password>
nmcli d wifi hotspot ifname wlx588694fa88ae ssid myraspi password 88888888
iwconfig wlp3s0 freq 2.4G

# disable touchpad
xinput set-prop 13 "Device Enabled" 0



